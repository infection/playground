{{ form_start(form) }}
    <div style="display: none">
        {{ form_widget(form) }}
    </div>


    <div class="flex">
        <div class="flex-1 text-gray-700 bg-gray-400 px-4 py-2 m-2">
            <div id="editor-code" style="height:400px;"></div>
        </div>
        <div class="flex-1 text-gray-700 bg-gray-400 px-4 py-2 m-2">
            <div id="editor-test" style="height:400px;"></div>
        </div>
        <div class="flex-1 text-gray-700 bg-gray-400 px-4 py-2 m-2">
            <div id="editor-config" style="height:400px;"></div>
        </div>
    </div>

    <button id="js-submit" class="bg-transparent hover:bg-teal-500 text-teal-700 font-semibold hover:text-white py-2 px-4 border border-teal-500 hover:border-transparent rounded" type="button">Run</button>
{{ form_end(form) }}
